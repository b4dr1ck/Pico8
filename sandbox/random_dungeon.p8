pico-8 cartridge // http://www.pico-8.com
version 16
__lua__



function _init()
    dungeon={}
    start_x=7
    start_y=7

    for y=0,15 do
        dungeon[y] = {}
        for x=0,15 do
            dungeon[y][x] = 1
        end
    end

     dungeon[start_y][start_x] = 0




end

function _update()

end

function _draw()
    cls()

    for y=0,15 do
        for x=0,15 do
            if dungeon[y][x] == 1 then
                spr(1,x*8,y*8)
            end
        end
    end

end

